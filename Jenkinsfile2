pipeline {
    agent any

    stages {
        stage('Checkout code') {
            steps {
                bat 'echo "Sample project fetched from git"'
            }
        }
        stage('Build') {
            steps {
                bat 'echo "Building bricks"'
            }
        }
        stage('Deployment') {
            steps {
                bat 'echo "Deploying"'
            }
        }
    }
}
